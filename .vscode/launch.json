{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "node: discord-gateway",
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "args": "workspace @blargbot/discord-gateway start",
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "node: discord-proxy",
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "args": "workspace @blargbot/discord-proxy start",
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "node: master",
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "args": "workspace @blargbot/master start",
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "node: cluster",
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "args": "workspace @blargbot/cluster start",
            "env": {
                "CLUSTER_ID": "0",
                "SHARDS_MAX": "1",
                "SHARDS_FIRST": "0",
                "SHARDS_LAST": "0"
            },
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "node: api",
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "args": "workspace @blargbot/api start",
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "name": "node: current",
            "type": "node",
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "program": "node",
            "request": "launch",
            "args": [
                "${fileDirname}/${fileBasenameNoExtension}.js"
            ],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "mocha: all",
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "program": "mocha",
            "args": [
                "--no-timeouts",
                "--config",
                "${workspaceFolder}/mocharc.json",
                "--recursive"
            ],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "mocha: current",
            "console": "integratedTerminal",
            "runtimeExecutable": "yarn",
            "program": "mocha",
            "args": [
                "--no-timeouts",
                "--config",
                "${workspaceFolder}/mocharc.json",
                "${fileDirname}/${fileBasenameNoExtension}.js"
            ],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
            ],
            "env": {
                "TS_NODE_FILES": "true"
            },
            "preLaunchTask": {
                "type": "npm",
                "script": "watch"
            }
        }
    ]
}