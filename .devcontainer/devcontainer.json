{
	"name": "Blargbot",
	"dockerComposeFile": "docker-compose.yaml",
	"service": "editor",
	"postStartCommand": "yarn",
	"workspaceFolder": "/workspaces/blargbot",
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"dbaeumer.vscode-eslint",
				"gamunu.vscode-yarn",
				"arcanis.vscode-zipfs",
				"eamodio.gitlens",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"initializeCommand": "cd .devcontainer && ./init",
	"features": {
		"ghcr.io/devcontainers/features/docker-from-docker:1": {}
	}
}